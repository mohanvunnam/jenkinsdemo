def putparam() {
                        list.each {
                                //echo "appname ${it.appname} and Version is ${it.version}"
                               // appname1 = it.appname
                                //version1 = it.version
    echo "Executing on Environment $environment. Values of Application = ${it.appname}, Pakage Version = ${it.version}"
    sh "ls -ltrh | tail -5"
    echo "aws ssm put-parameter --name /${environment}/terraform/docker_images/${it.appname}_image --value 753224138664.dkr.ecr.us-east-1.amazonaws.com/${it.appname}:${it.version} --overwrite  --type String"
} // End of List
} // End of Put Functions

def deployApp() {
		echo "calling deployApp function"
} // End of deployApp Functions


pipeline {
    agent any
    stages {


       stage('Read the variables of service and versions') {
          steps {
                script {
		def datas = readYaml file: 'serviceskeyval.yml'
		def approval=datas."${environment}".approval
		def tfbranch=datas."${environment}".tfbranch
		def nbr_api_ec2=datas."${environment}".nbr_api_ec2
		nbr_ui_ec21=datas."${environment}".nbr_ui_ec2
                cybersource_loopback1=datas."${environment}".cybersource_loopback
                dc_dcu_loopback=datas."${environment}".dc_dcu_loopback
                dc_dna_3rivers_loopback=datas."${environment}".dc_dna_3rivers_loopback
                dc_dna_meritrust_loopback=datas."${environment}".dc_dna_meritrust_loopback
                dc_cocc_reading_loopback=datas."${environment}".dc_cocc_reading_loopback
                dc_mahalo_profed_loopback=datas."${environment}".dc_mahalo_profed_loopback
                dc_episys_coastal_loopback=datas."${environment}".dc_episys_coastal_loopback
                dc_episys_dover_loopback=datas."${environment}".dc_episys_dover_loopback
                dc_episys_publix_loopback=datas."${environment}".dc_episys_publix_loopback
                dc_episys_unitus_loopback=datas."${environment}".dc_episys_unitus_loopback
                dc_finxact_primis_loopback=datas."${environment}".dc_finxact_primis_loopback
                dc_q2_mercantile_loopback=datas."${environment}".dc_q2_mercantile_loopback
                rce_worker_loopback=datas."${environment}".rce_worker_loopback
                venmo_loopback=datas."${environment}".venmo_loopback
                bpx_loopback=datas."${environment}".bpx_loopback
                dc_cocc_libertybank_loopback=datas."${environment}".dc_cocc_libertybank_loopback
                dc_xp2_nusenda_loopback=datas."${environment}".dc_xp2_nusenda_loopback
                dc_episys_wrightpatt_loopback=datas."${environment}".dc_episys_wrightpatt_loopback
                dc_episys_eecu_loopback=datas."${environment}".dc_episys_eecu_loopback
                dc_q2_firstnorthernbank_loopback=datas."${environment}".dc_q2_firstnorthernbank_loopback
                dc_q2_5star_loopback=datas."${environment}".dc_q2_5star_loopback
                dc_finxact_glorifi_loopback=datas."${environment}".dc_finxact_glorifi_loopback
                dc_keystone_cenwill_loopback=datas."${environment}".dc_keystone_cenwill_loopback
                dc_episys_hudsonvalley_loopback=datas."${environment}".dc_episys_hudsonvalley_loopback
                dc_episys_kinecta_loopback=datas."${environment}".dc_episys_kinecta_loopback
                dc_episys_georgiaunited_loopback=datas."${environment}".dc_episys_georgiaunited_loopback
                dc_q2_chesapeakebank_loopback=datas."${environment}".dc_q2_chesapeakebank_loopback
                dc_episys_airforce_loopback=datas."${environment}".dc_episys_airforce_loopback
                dc_episys_cport_loopback=datas."${environment}".dc_episys_cport_loopback
                dc_keystone_expedition_loopback=datas."${environment}".dc_keystone_expedition_loopback
                dc_episys_srp_loopback=datas."${environment}".dc_episys_srp_loopback
                dc_q2_advia_loopback=datas."${environment}".dc_q2_advia_loopback
                dc_dna_georgiasown_loopback=datas."${environment}".dc_dna_georgiasown_loopback
                dc_portico_trailnorth_loopback=datas."${environment}".dc_portico_trailnorth_loopback
                dc_cunify_oregonians_loopback=datas."${environment}".dc_cunify_oregonians_loopback
                dc_keystone_frontwave_loopback=datas."${environment}".dc_keystone_frontwave_loopback
                      
                list=datas."${environment}".applist

                       } //End of Script
                } //End of steps
                                                            } //End of Stage - Read the variables of service and versions


       stage('Execute based on Environment') {
          steps {
                script {
                        if (environment == 'prod' || environment == 'imp1' || environment == 'conv') {
                                putparam()

                            } else if (environment == 'dev1' || environment == 'dev2' ||  environment == 'dev3' || environment == 'qa1' ) {
                                putparam()
                            } else if (environment == 'pref' || environment == 'qa2') {
                                putparam()
                            } else if (environment == 'uat1') {
                                putparam()
                            } else {
                                echo 'No Matching environment found....... '
                                   }
                       }
                }
                                                                }


//	stage ('create ssm parameter store api'){
//            when {
//               environment name: 'update_api', value: 'yes'
//                 }// End of When

//            steps {
//                script {
//			putparam()
//		       } //End of Script
//		  } //End of Steps
//					}//End of stage - create ssm parameter store api


	stage ('terraform'){
            steps {
		echo "dc_dcu_loopback is ${dc_dcu_loopback} and dc_mahalo_profed_loopback is ${dc_mahalo_profed_loopback} and dc_cunify_oregonians_loopback is ${dc_cunify_oregonians_loopback} and cybersource_loopback is ${cybersource_loopback1}"
               dir("/tmp/environments/main/infrastructure") {
                    sh '''
                      echo  "env0 deploy \
                        --environmentName infrastructure-${environment} \
                        --workspaceName ${environment} \
                        --requiresApproval ${approval}\
                        --revision ${tfbranch} \
                        --environmentVariables ENV0_TERRAFORM_BACKEND_CONFIG=devbackend \
TF_VAR_cybersource_loopback=${cybersource_loopback1} TF_VAR_dc_dcu_loopback=${dc_dcu_loopback} TF_VAR_dc_dna_3rivers_loopback=${dc_dna_3rivers_loopback} TF_VAR_dc_dna_meritrust_loopback=${dc_dna_meritrust_loopback} TF_VAR_dc_cocc_reading_loopback=${dc_cocc_reading_loopback} TF_VAR_dc_mahalo_profed_loopback=${dc_mahalo_profed_loopback} TF_VAR_dc_episys_coastal_loopback=${dc_episys_coastal_loopback} TF_VAR_dc_episys_dover_loopback=${dc_episys_dover_loopback} TF_VAR_dc_episys_publix_loopback=${dc_episys_publix_loopback} TF_VAR_dc_episys_unitus_loopback=${dc_episys_unitus_loopback} TF_VAR_dc_finxact_primis_loopback=${dc_finxact_primis_loopback} TF_VAR_dc_q2_mercantile_loopback=${dc_q2_mercantile_loopback} TF_VAR_rce_worker_loopback=${rce_worker_loopback} TF_VAR_venmo_loopback=${venmo_loopback} TF_VAR_bpx_loopback=${bpx_loopback} TF_VAR_dc_cocc_libertybank_loopback=${dc_cocc_libertybank_loopback} TF_VAR_dc_xp2_nusenda_loopback=${dc_xp2_nusenda_loopback} TF_VAR_dc_episys_wrightpatt_loopback=${dc_episys_wrightpatt_loopback} TF_VAR_dc_episys_eecu_loopback=${dc_episys_eecu_loopback} TF_VAR_dc_q2_firstnorthernbank_loopback=${dc_q2_firstnorthernbank_loopback} TF_VAR_dc_q2_5star_loopback=${dc_q2_5star_loopback} TF_VAR_dc_finxact_glorifi_loopback=${dc_finxact_glorifi_loopback} TF_VAR_dc_keystone_cenwill_loopback=${dc_keystone_cenwill_loopback} TF_VAR_dc_episys_hudsonvalley_loopback=${dc_episys_hudsonvalley_loopback} TF_VAR_dc_episys_kinecta_loopback=${dc_episys_kinecta_loopback} TF_VAR_dc_episys_georgiaunited_loopback=${dc_episys_georgiaunited_loopback} TF_VAR_dc_q2_chesapeakebank_loopback=${dc_q2_chesapeakebank_loopback} TF_VAR_dc_episys_airforce_loopback=${dc_episys_airforce_loopback} TF_VAR_dc_episys_cport_loopback=${dc_episys_cport_loopback} TF_VAR_dc_keystone_expedition_loopback=${dc_keystone_expedition_loopback} TF_VAR_dc_episys_srp_loopback=${dc_episys_srp_loopback} TF_VAR_dc_q2_advia_loopback=${dc_q2_advia_loopback} TF_VAR_dc_dna_georgiasown_loopback=${dc_dna_georgiasown_loopback} TF_VAR_dc_portico_trailnorth_loopback=${dc_portico_trailnorth_loopback} TF_VAR_dc_cunify_oregonians_loopback=${dc_cunify_oregonians_loopback} TF_VAR_dc_keystone_frontwave_loopback=${dc_keystone_frontwave_loopback} \
                        TF_VAR_spotinst_api_desired_capacity=${nbr_api_ec2} \
                        TF_VAR_spotinst_ui_desired_capacity=${nbr_ui_ec2} "
                    '''
                }
            }
        }


} // End of Stages
} // End of Pipeline
