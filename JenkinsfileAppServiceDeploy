//import groovy.json.JsonSlurper

pipeline {
    agent any
    stages {
       stage('Read the variables of service and versions') {
          steps {
                script {
		def datas = readYaml file: 'service.yml'
		httpq=datas.ENVIRONMENT.httpq
		webhook=datas.ENVIRONMENT.webhook
		apimgr=datas.ENVIRONMENT.apimgr
                echo "Got  httpq as ${httpq}"
                echo "Got  webhook as ${webhook}"
                echo "Got  appmgr as ${appmgr}"
                       }
                }
                                                                }



}
}
